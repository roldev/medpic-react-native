{
    "expo": {
        "name": "medpic",
        "description": "Send ECG pictures for machine learning diagnosis",
        "slug": "medpic",
        "version": "1.1.0",
        "orientation": "portrait",
        "icon": "./assets/icon.png",
        "splash": {
            "image": "./assets/splash.png",
            "resizeMode": "contain",
            "backgroundColor": "#ffffff"
        },
        "updates": {
            "checkAutomatically": "ON_LOAD",
            "fallbackToCacheTimeout": 300000
        },
        "assetBundlePatterns": [
            "**/*"
        ],
        "ios": {
            "buildNumber": "1.1.0",
            "supportsTablet": true,
            "bundleIdentifier": "com.medpic.app"
        },
        "android": {
            "versionCode": 1,
            "package": "com.medpic.app",
            "adaptiveIcon": {
                "foregroundImage": "./assets/adaptive-icon.png",
                "backgroundColor": "#FFFFFF"
            }
        },
        "web": {
            "favicon": "./assets/favicon.png"
        }
    },
    "builds": {
        "android": {
            "release": {
                "workflow": "managed"
            }
        },
        "ios": {
            "release": {
                "workflow": "managed"
            }
        }
    }
}